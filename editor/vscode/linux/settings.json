// Place your settings in this file to overwrite the default settings
{
    "editor.codeLens": true,
    "editor.cursorBlinking": "smooth",
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": true,
    "editor.tabSize": 2,
    "editor.renderWhitespace": "all",
    "workbench.colorCustomizations": {
      "editorWhitespace.foreground": "#1e1e1e"
    },
    "guides.active.color.dark": "rgba(120, 120, 120, 0.75)",
    "guides.active.hideOnSelection": false,
    "guides.active.style": "dashed",
    "guides.indent.hideBackgroundOnSelection": false,
    "guides.normal.color.dark": "rgba(100, 100, 100, 0.75)",
    "guides.normal.hideOnSelection": false,
    "guides.normal.style": "dashed",
    "guides.stack.hideOnSelection": false,
    "guides.stack.style": "dashed",
    "guides.updateDelay": 0.01,
    "http.proxyStrictSSL": false,
    "terminal.integrated.fontFamily": "Cousine for Powerline",
    "terminal.integrated.lineHeight": 1.1,
    "typescript.referencesCodeLens.enabled": true,
    "vsicons.projectDetection.disableDetect": true,
    "window.zoomLevel": 0,
    "workbench.iconTheme": "vscode-icons",
    "editor.fontFamily": "'Cousine for Powerline', Consolas, 'Courier New', monospace",
    "git.autofetch": false,
    "editor.minimap.showSlider": "always",
    "editor.renderIndentGuides": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "gitlens.advanced.messages": {
      "suppressCommitHasNoPreviousCommitWarning": false,
      "suppressCommitNotFoundWarning": false,
      "suppressFileNotUnderSourceControlWarning": false,
      "suppressGitVersionWarning": false,
      "suppressLineUncommittedWarning": false,
      "suppressNoRepositoryWarning": false,
      "suppressUpdateNotice": false,
      "suppressWelcomeNotice": true
    },
    "window.menuBarVisibility": "default",
    "files.eol": "\r\n",
    "explorer.confirmDragAndDrop": false,
    "[go]": {
      "editor.insertSpaces": false,
      "editor.formatOnSave": true,
      "editor.tabSize": 4
    },
    "editor.matchBrackets": false,
    "gitlens.keymap": "alternate",
    "extension-manager.extensions": [
      "bibhasdn.unique-lines",
      "bierner.markdown-preview-github-styles",
      "DanielThielking.aws-cloudformation-yaml",
      "darkriszty.markdown-table-prettify",
      "DavidAnson.vscode-markdownlint",
      "donjayamanne.githistory",
      "eamodio.gitlens",
      "jchannon.csharpextensions",
      "jesschadwick.nuget-reverse-package-search",
      "k--kato.docomment",
      "lukehoban.Go",
      "mechatroner.rainbow-csv",
      "ms-vscode.csharp",
      "ms-vscode.PowerShell",
      "pajoma.vscode-journal",
      "rafamel.subtle-brackets",
      "robertohuertasm.vscode-icons",
      "slevesque.vscode-autohotkey",
      "spywhere.guides",
      "yzhang.markdown-all-in-one"
    ]
  }
